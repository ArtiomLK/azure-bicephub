        "rg-envision-dev-westus3": {
          "sub_id": "<sub_id>",
          "location": "westus3"
        }
      }
    },

    "plans": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus",
          "name": "plan-envision-dev-eastus",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        },
        {
          "rg_n": "rg-envision-dev-westus3",
          "name": "plan-envision-dev-westus3",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        }
      ]
    },

    "logs": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus",
          "name": "log-envision-dev-eastus"
        },
        {
          "rg_n": "rg-envision-dev-westus3",
          "name": "log-envision-dev-westus3"
        }
      ]
    },

    "appis": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus",
          "name": "appi-envision-dev-eastus",
          "log_id": "<sub_id>/resourcegroups/rg-envision-dev-eastus/providers/microsoft.operationalinsights/workspaces/log-envision-dev-eastus"
        },
        {
          "rg_n": "rg-envision-dev-westus3",
          "name": "appi-envision-dev-westus3",
          "log_id": "<sub_id>/resourcegroups/rg-envision-dev-westus3/providers/microsoft.operationalinsights/workspaces/log-envision-dev-westus3"
        }
      ]
    },

    "apps": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus",
          "plan_id": "<sub_id>/resourceGroups/rg-envision-dev-eastus/providers/Microsoft.Web/serverFarms/plan-envision-dev-eastus",
          "name": "app-a-envision-dev-eastus-alk",
          "appi": {
            "rg_n": "rg-envision-dev-eastus",
            "name": "appi-envision-dev-eastus"
          }
