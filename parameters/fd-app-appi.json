        {
          "rg_n": "rg-envision-dev-westus3",
          "plan_id": "<sub_id>/resourceGroups/rg-envision-dev-westus3/providers/Microsoft.Web/serverFarms/plan-envision-dev-westus3",
          "name": "app-a-envision-dev-westus3-alk",
          "appi": {
            "rg_n": "rg-envision-dev-westus3",
            "name": "appi-envision-dev-westus3"
          }
        }
      ]
