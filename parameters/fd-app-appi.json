{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "tags": {
      "value": {
        "project": "bicephub",
        "architecture": "fd-app-appi",
        "env": "dev"
      }
    },

    "rgs": {
      "value": {
        "rg-envision-dev-eastus2": {
          "sub_id": "<sub_id>",
          "location": "eastus2"
        },
        "rg-envision-dev-centralus": {
          "sub_id": "<sub_id>",
          "location": "centralus"
        }
      }
    },

    "plans": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus2",
          "name": "plan-envision-dev-eastus2",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        },
        {
          "rg_n": "rg-envision-dev-centralus",
          "name": "plan-envision-dev-centralus",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        }
      ]
    },

    "logs": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus2",
          "name": "log-envision-dev-eastus2"
        },
        {
          "rg_n": "rg-envision-dev-centralus",
          "name": "log-envision-dev-centralus"
        }
      ]
    },

    "appis": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus2",
          "name": "appi-envision-dev-eastus2",
          "log_id": "<sub_id>/resourcegroups/rg-envision-dev-eastus2/providers/microsoft.operationalinsights/workspaces/log-envision-dev-eastus2"
        },
        {
          "rg_n": "rg-envision-dev-centralus",
          "name": "appi-envision-dev-centralus",
          "log_id": "<sub_id>/resourcegroups/rg-envision-dev-centralus/providers/microsoft.operationalinsights/workspaces/log-envision-dev-centralus"
        }
      ]
    },

    "apps": {
      "value": [
        {
          "rg_n": "rg-envision-dev-eastus2",
          "plan_id": "<sub_id>/resourceGroups/rg-envision-dev-eastus2/providers/Microsoft.Web/serverFarms/plan-envision-dev-eastus2",
          "name": "app-a-envision-dev-eastus2",
          "appi": {
            "rg_n": "rg-envision-dev-eastus2",
            "name": "appi-envision-dev-eastus2"
          }
        },
        {
          "rg_n": "rg-envision-dev-eastus2",
          "plan_id": "<sub_id>/resourceGroups/rg-envision-dev-eastus2/providers/Microsoft.Web/serverFarms/plan-envision-dev-eastus2",
          "name": "app-b-envision-dev-eastus2",
          "appi": {
            "name": "appi-envision-dev-eastus2"
          }
        },
        {
          "rg_n": "rg-envision-dev-eastus2",
          "plan_id": "<sub_id>/resourceGroups/rg-envision-dev-eastus2/providers/Microsoft.Web/serverFarms/plan-envision-dev-eastus2",
          "name": "app-c-envision-dev-eastus2"
        },

        {
          "rg_n": "rg-envision-dev-centralus",
          "plan_id": "<sub_id>/resourceGroups/rg-envision-dev-centralus/providers/Microsoft.Web/serverFarms/plan-envision-dev-centralus",
          "name": "app-a-envision-dev-centralus",
          "appi": {
            "rg_n": "rg-envision-dev-centralus",
            "name": "appi-envision-dev-centralus"
          }
        }
      ]
    },

    "fd": {
      "value": {
        "rg_n": "rg-envision-dev-eastus2",
        "name": "fd-envision-dev",
        "sku": "Premium_AzureFrontDoor",
        "route_n": "envision-prod-route",
        "origin_g_n": "envision-prod-origin-group",
        "origin_g_health_probe_settings": "Https"
      }
