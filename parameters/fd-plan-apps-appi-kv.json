      ]
    },

    "apps": {
      "value": [
        {
          "rg_n": "rg-invent-dev-eastus2",
          "plan_id": "<sub_id>/resourceGroups/rg-invent-dev-eastus2/providers/Microsoft.Web/serverFarms/plan-invent-dev-eastus2",
