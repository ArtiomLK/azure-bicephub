          "appi": {
            "rg_n": "rg-invent-dev-eastus2",
            "name": "appi-invent-eastus2-dev"
          }
        },

        {
          "rg_n": "rg-invent-dev-centralus",
          "plan_id": "<sub_id>/resourceGroups/rg-invent-dev-centralus/providers/Microsoft.Web/serverFarms/plan-invent-dev-centralus",
          "name": "app-a-invent-centralus-dev",
          "appi": {
            "rg_n": "rg-invent-dev-centralus",
            "name": "appi-invent-centralus-dev"
          },
          "kv_id": "<sub_id>/resourceGroups/rg-invent-dev-centralus/providers/Microsoft.KeyVault/vaults/kv-invent-centralus-dev"
        }
      ]
    },

    "fd": {
      "value": {
        "rg_n": "rg-invent-dev-eastus2",
        "name": "fd-invent-dev",
        "sku": "Premium_AzureFrontDoor",
        "route_n": "invent-prod-route",
        "origin_g_n": "invent-prod-origin-group",
        "origin_g_health_probe_settings": "Https"
      }
    }
