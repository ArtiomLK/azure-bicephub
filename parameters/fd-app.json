{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "tags": {
      "value": {
        "project": "bicephub",
        "architecture": "fd-app",
        "env": "dev"
      }
    },

    "rgs": {
      "value": {
        "rg-imagine-dev-eastus": {
          "sub_id": "<sub_id>",
          "location": "eastus"
        },
        "rg-imagine-dev-westus3": {
          "sub_id": "<sub_id>",
          "location": "westus3"
        }
      }
    },

    "plans": {
      "value": [
        {
          "rg_n": "rg-imagine-dev-eastus",
          "name": "plan-imagine-dev-eastus",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        },
        {
          "rg_n": "rg-imagine-dev-westus3",
          "name": "plan-imagine-dev-westus3",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        }
      ]
    },

    "apps": {
      "value": [
        {
          "rg_n": "rg-imagine-dev-eastus",
          "plan_id": "<sub_id>/resourceGroups/rg-imagine-dev-eastus/providers/Microsoft.Web/serverFarms/plan-imagine-dev-eastus",
          "name": "app-a-imagine-dev-eastus-alk"
        },

        {
          "rg_n": "rg-imagine-dev-westus3",
          "plan_id": "<sub_id>/resourceGroups/rg-imagine-dev-westus3/providers/Microsoft.Web/serverFarms/plan-imagine-dev-westus3",
          "name": "app-a-imagine-dev-westus3-alk"
        }
      ]
    },

    "fd": {
      "value": {
        "rg_n": "rg-imagine-dev-eastus",
        "name": "fd-imagine-dev",
        "sku": "Premium_AzureFrontDoor",
        "route_n": "imagine-prod-route",
        "origin_g_n": "imagine-prod-origin-group",
        "origin_g_health_probe_settings": "Https"
      }
    }
  }
}
