        "rg-invent-dev-westus3": {
          "sub_id": "<sub_id>",
          "location": "westus3"
        }
      }
    },

    "kvs": {
      "value": [
        {
          "rg_n": "rg-invent-dev-eastus",
          "name": "invent-dev-eastus",
          "enable_rbac": "true",
          "sku": "standard"
        },
        {
          "rg_n": "rg-invent-dev-westus3",
          "name": "invent-dev-westus3",
          "enable_rbac": "true",
          "sku": "standard"
        }
      ]
    },

    "plans": {
      "value": [
        {
          "rg_n": "rg-invent-dev-eastus",
          "name": "plan-invent-dev-eastus",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        },
        {
          "rg_n": "rg-invent-dev-westus3",
          "name": "plan-invent-dev-westus3",
          "sku_code": "B1",
          "sku_tier": "Basic",
          "os_kind": "windows",
          "enable_zone_redundancy": false
        }
      ]
    },

