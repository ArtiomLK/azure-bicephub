{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "tags": {
      "value": {
        "project": "bicephub",
        "architecture": "fd-apps-appi-kv",
        "env": "dev"
      }
    },

    "rgs": {
      "value": {
        "rg-invent-dev-eastus": {
          "sub_id": "<sub_id>",
