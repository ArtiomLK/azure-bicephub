        {
          "rg_n": "rg-invent-dev-westus3",
          "name": "log-invent-dev-westus3"
        }
      ]
    },

    "appis": {
      "value": [
        {
          "rg_n": "rg-invent-dev-eastus",
          "name": "appi-invent-dev-eastus",
          "log_id": "<sub_id>/resourcegroups/rg-invent-dev-eastus/providers/microsoft.operationalinsights/workspaces/log-invent-dev-eastus"
        },
        {
          "rg_n": "rg-invent-dev-westus3",
          "name": "appi-invent-dev-westus3",
          "log_id": "<sub_id>/resourcegroups/rg-invent-dev-westus3/providers/microsoft.operationalinsights/workspaces/log-invent-dev-westus3"
